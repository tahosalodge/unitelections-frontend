{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@date-io/date-fns": "^1.0.3",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@paralleldrive/react-feature-toggles": "^2.1.0",
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^4.5.3",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.26",
    "date-fns-tz": "^1.0.6",
    "formik": "^1.4.3",
    "lodash": "^4.17.11",
    "material-ui-pickers": "^2.1.1",
    "notistack": "^0.4.1",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-ga": "^2.5.7",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.1.3",
    "yup": "^0.26.10"
  },
  "scripts": {
    "start": "NODE_PATH=src/ react-scripts start",
    "build": "NODE_PATH=src/ react-scripts build",
    "build:with-stats": "NODE_PATH=src react-scripts build --stats",
    "analyze": "webpack-bundle-analyzer build/bundle-stats.json -m server",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-redux-saga": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier-eslint": "^8.8.2",
    "webpack-bundle-analyzer": "^3.0.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
